html{
	background-color: #f5f5f5;
	color: #363d4d;
	height: 100%;
	text-align: center;
}
body{
	
	background-size: 100% 100%;
	height: 100%;
}

h1{color: #363d4d;}
h2{color: #5c6bc2}
.wrapper{
	padding: 5% 0;
}
.timu-list{
	text-align: left;
	margin-top: 1rem;
}

.tip{
	color: #b6b6b6;
	display: inline-block;
}

input{
    height: 2.5rem;
    border: 1px solid #ccc;
    border-radius: 3px;
    padding: 0 2%;
}
.timu-list h1{
	display: inline-block;
}
.timu-list p span{vertical-align: middle;}
.timu-list > li > p{margin-bottom: 10px;font-size: 16px;}
.timu-list ol{
	margin-left: 10%;
}
.timu-list ol li{
	display: inline-block;
	margin-top: 20px;
}
.timu-list > li{
	background-color: #fff;
	padding: 1rem 3%;
	border-bottom: 1px solid #e8e8e8;
	margin-bottom: 10px;
}
.choose-input input{
	width: 92%;
}
.light-option .choose-muti>span{
	width: 95%;
	line-height: 30px;
	margin-top: 5px;
}
/*多选题*/
.choose-muti,.choose-single,.choose-input,.choose-textarea{
	margin-left: 10%;
}

.choose-muti span{
	display: inline-block;
	/*padding: 3px 10px;*/
	margin-right: 15px;
	border-radius: 8px;
	font-size: 14px;
}
.choose-M-no{
	color: #363d4d;
	/*background-color: #f5f5f5;*/
	position: relative;
}
.choose-M-yes{
	color: #afbaef;
	/*background-color: #afbaef;*/
	position: relative;
}
/*单选题*/
.choose-single p{
	margin-bottom: 5px;
}
.choose-S-yes{
	color: #afbaef;
}
.choose-S-no{
	color: #363d4d;
}
.choose-single input{
	margin-left: 1rem;
	min-width: 80%;
}
/*文本框*/
textarea{
	width: 90%;
    min-height: 100px;
    border: 1px solid #ccc;
    border-radius: 5px;
    padding: 2%;
}
/*缩略图*/
.icon-thumb{
	position: absolute;
	right: 5%;
	width: 30px;
	z-index: 10;
	top: 25%;
}

/*button*/
button{
	/*width: 80%;*/
	background-color: #5c6bc2;
	padding: 10px 0;
	border:none;
	
	font-size: 17px;
	color: #fff;
	border-radius: 5px;
}
.wrapper-preview{
	display: none;
}
.btn-wrapper-preview button{
	width: 80%;
}
.wrapper-submit{
	display: none;
}
.btn-wrapper-submit button{
	width: 40%;
	margin: 0 10px;
}
.btn-wrapper-preview,.btn-wrapper-submit{
	padding: 20px 0;
    background-color: #fff;
}
/*preview*/

.wrapper-preview .choose-single p,.wrapper-preview .choose-muti p{
	color: #b6b6b6;
}
/*提交成功提示语*/
.tip-succ{display: none;}
/*提示*/

.tips{
	color: #f10;
	font-size: 12px;
	margin-left: 10%;
	/*display: none;*/
	opacity: 0;
}
/*特效弹出框*/

.img-msk{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	background-color: rgba(0,0,0,.6);
	z-index: -1;
}
.pop-img p{
	color: #fff;
    margin-top: 20%;
    margin-bottom: 10%;
}
.pop-img{
	position: fixed;
	z-index: 1000;
	width: 100%;
	height: 100%;
	text-align: center;
	display: none;
}
.pop-img img{
	width: 60%;

}
