*{margin:0;padding: 0}
input{
    border: 0px;
    outline: none;
}
body{
	max-width: 640px;
	margin:0 auto;
	position: relative;
}
[v-cloak]{
    display: none;
}
.wrap{
    background: url(../img-speak/bg.jpg) no-repeat;
    background-size:cover; 
    position: relative;
    padding: 1.125rem .83rem 1.66rem;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.wrap_sole{
    position: relative;
}
.wrap_sole>img{
    margin: 0 -0.83rem;
    width: calc(100% + 1.66rem);
}
.wrap_sole .sole_rule{
    position: absolute;
    top: 1.46rem;
    right: 0;
}
.wrap_sole .bidding{
    display: flex;
    flex-flow: column-reverse ;
}
.sole_rule img,.wrap_tags_img img{
    width: 7rem;
    height: 2.2rem;
    display: block;
    margin-bottom: 0.91rem;
}
.sole_rule .bidding_img{
    display: none;
}
.wrap_sole .bidding .bidding_img{
    display: block;
}
/*选择偶像，输入寄语*/
.wrap_idol{
    background: #fff;
    border-radius: .83rem;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    height: 12.25rem;
    margin-top: .83rem;
    padding: 1.25rem 0.91rem 0 ;
    font-size: 1.3rem;
}
.wrap_idol_top_head{
    margin-bottom: 1rem;
}
.wrap_idol_top_head .head{
    width: 4.16rem;
    height: 4.16rem;
    border-radius: 50%;
    display: inline-block;
    border: 1px solid #f3f3f3;
}
.wrap_idol_top_head .name{
    margin-left: .83rem;
    font-weight: bold;
    color: #035ebd;
}

.wrap_idol_top_head .name2{
    color: #4ec1b0;
}

.wrap_idol_bottom{
    border-radius: .75rem; 
}
.wrap_idol_bottom input{
    text-align: center;
    background: #f6f6f6;
    border-radius: .75rem; 
    width: 100%;
    height: 3.95rem;
    border: 1px solid #f0f0f0;
    line-height: 3.95rem;
}
.wrap_idol_bottom .sendWord{
    font-size: 2.5rem;
    text-align: center;
    font-weight: 500;
}
/*竞价*/
.wrap_bidding{
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    margin-top: .83rem;
    -webkit-box-pack: justify;
        -ms-flex-pack: justify;
            justify-content: space-between;
}
.wrap_bidding_left{
    background: #ccc;
    border-radius: .83rem;
    /* width: 18rem; */
    width: 68%;
}
.wrap_bidding_left input{
    /* width: 18rem; */
    width: 100%;
    height: 3.66rem;
    line-height: 3.66rem;
    background: #fff;
    border-radius: .83rem;
    text-align: center;
    font-weight: bold;
}
.wrap_bidding_right{
    width: 30%;
    /* width: 10.83rem; */
    background: url(../img-speak/bidding_bg.png) no-repeat ;
    background-size:100% 100%;
    border-radius: .83rem;
    -webkit-box-shadow: 0px 2px 4px #0341af;
            box-shadow: 0px 2px 4px #0341af;
    text-align: center;
    line-height: 3.66rem;
}
.wrap_bidding_right img{
    width:83%;
}
/*个性化贴纸*/
.wrap_tags{
    background: url(../img-speak/tags_bg.png) no-repeat;
    background-size:100% 100%; 
    height: 8rem;
    margin-top:.83rem; 
    position: relative;
}
.wrap_tags .wrap_tags_img{
    position: absolute;
    right: 0;
    top: .75rem;
}
.wrap_tags_img img{
    display: block;
    margin-top:.7rem;
}
/*运动项目*/
.wrap_exercise{
    margin-top: .7rem;
}
.wrap_exercise ul li{ 
    height: 5rem;
    margin-top: .58rem;
    padding: .67rem 1.2rem 0 1.95rem;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    color: #fff;
}
.wrap_exercise ul li>div{
    width:100%;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
}
.wrap_exercise_li_left{
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-flex: 1;
        -ms-flex: 1;
            flex: 1;
}
.wrap_exercise_li_left img{
    border-radius: 50%;
    border: 1px soli #fff;
    width: 3.33rem;
    height: 3.33rem;
    margin-right: .54rem;
}
.wrap_exercise_li_left .idolName{
    font-size: 1.4rem;
    font-weight: bold;
}
.wrap_exercise_li_left .items{
    font-size: 1.2rem
}
.wrap_exercise_li_right{
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: reverse;
        -ms-flex-flow: row-reverse;
            flex-flow: row-reverse;
    -webkit-box-flex: 1;
        -ms-flex: 1;
            flex: 1;
}
.wrap_exercise_li_right img{
    width: 3.25rem;
    height: 3.25rem;
    margin-left: .67rem;
}
.wrap_exercise_li_right img:nth-last-of-type(1){
    margin-left: 0;
}
.wrap_exercise ul li:nth-child(1){
    background: url(../img-speak/exercise_1.png) no-repeat;
    background-size: 100% 100%;
    margin-top:0;
}
.wrap_exercise ul li:nth-child(2){
    background: url(../img-speak/exercise_2.png) no-repeat;
    background-size: 100% 100%;
}
.wrap_exercise ul li:nth-child(3){
    background: url(../img-speak/exercise_3.png) no-repeat;
    background-size: 100% 100%;
}
.wrap_exercise ul li:nth-child(4){
    background: url(../img-speak/exercise_4.png) no-repeat;
    background-size: 100% 100%;
}
.wrap_exercise ul li:nth-child(5){
    background: url(../img-speak/exercise_5.png) no-repeat;
    background-size: 100% 100%;
}
.wrap_exercise ul li:nth-child(6){
    background: url(../img-speak/exercise_6.png) no-repeat;
    background-size: 100% 100%;
}
.wrap_exercise ul li:nth-child(7){
    background: url(../img-speak/exercise_7.png) no-repeat;
    background-size: 100% 100%;
}
