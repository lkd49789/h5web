.mask {
    position: absolute;
    top:0;
    left:50%;
    width: 100%;
    height:100%;
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    transform: translateX(-50%);
    max-width: 750px;
    background:rgba(0,0,0,.3);
    display: none;
}
.mask img{
    width: 100%;
}
