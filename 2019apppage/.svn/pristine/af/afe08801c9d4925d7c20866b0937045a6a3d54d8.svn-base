    padding: 0 1em;
}
.share_text{
    background: url("../images/text_bg.png") no-repeat 0 0/100% 100%;
    padding: 12%;
    font-size: 18px;
}
.share_text .p{
    color:#ff8a70;
    margin-top: 5%;
}
.share_text .p:last-of-type{
