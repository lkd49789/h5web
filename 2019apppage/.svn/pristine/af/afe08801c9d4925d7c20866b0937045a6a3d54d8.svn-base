
html,body{
    height:100%;
}
body{
    max-width: 750px;
    margin: 0 auto;
    /*background-color: #011016;*/
    background-color: #f7f7f7;
    -webkit-overflow-scrolling: touch;
	font-size:10%;
}
.box {
    max-width: 750px;
    margin:0 auto;
}
.box img{
	width:100%;
}
.bg1{ position: relative;}
.bg1 img.b4_btn1{width:45%; margin:1.5em auto 4em;}
.bg1_con{background:url("../images/bg1.jpg") no-repeat;
    background-size:100% 100%; overflow: hidden; padding-bottom: 25%;padding: 0 .75em}
.text{background:url("../images/text_bg.png") no-repeat;background-size:100% 100%;padding:1.5em;font-size: 2.6em;margin-bottom: .4rem;}
.flex{display: flex;margin-bottom: .6em;}
.flex:last-of-type{margin-bottom: 0em;}
.text .color{color: #ff6d4e;flex: 1;}
.flex div:last-of-type{flex: 10;}
/*通用标题样式*/
.b_bt{margin: 3% auto 5%; }

.dy{font-size: 2.2em; line-height: 1.8; color: #fff; margin: 3% 0;padding: 0 8%; box-sizing: content-box;}
.group_btn_bg{background: url("../images/group_btn_icon.png") left top no-repeat;background-size:contain;}
.group_btn{width: 76.13%; margin: 0 auto; position: relative;}
.group_btn .url{ position: absolute; top:0px;left: 0px;width: 100%;}
.group_btn .url_a{float: left; width:33.33%;height: 150px;}

.bg2{margin-top: -18%; position: relative;}
.bg2_con{background: url("../images/bg2.png") center top no-repeat; background-size: cover; overflow: hidden; padding-bottom: 25%;}
.b2_con{width:87.2%; margin: 0 auto;}

.bg3{margin-top: -18%; position: relative;}
.bg3 .b_bt{margin-top: 6%;}
.bg3_con{background: url("../images/bg3.png") center top no-repeat; background-size: cover; overflow: hidden; padding-bottom: 25%;}
.b3_con1{width:84.4%; margin: 0 auto;}
.b3_con2{width:78%; margin: 0 auto;}

.bg4{margin-top: -18%; position: relative;}
.bg4_con{background: url("../images/bg1.png") center top no-repeat; background-size: cover; overflow: hidden; padding-bottom: 10%;}
.b4_con{width:80.8%; margin: 0 auto;}
.b4_btn_con{text-align: center; margin: 8% 0;}
.b4_btn_con img.b4_btn2{width:24.13%; margin-top: 3%;}
.b4_btn_con img.b4_btn3{width:24.13%; margin: 3% 1.5% 0;}
.b4_btn_con img.b4_btn4{width:32.13%; margin-top: 3%;}

.footer{margin: 10% 0 0;}

.q_btn_con{position: fixed; bottom: 0px; width:100%; max-width: 750px; text-align: center; color: #fff; font-size: 2.6em; background: #5063ca; box-shadow: 0 -10px 15px rgba(0,0,0,.6); box-sizing:border-box;}
.q_btn_con .btn{padding: 0% 2%; margin: 2.5% 0; color: #fff; font-weight: bold; display: inline-block; border-right: 1px dotted #fff;}
.q_btn_con .btn:last-child{ border-right:none;}

.email_plan{background: #f7f7f7; border-radius: 20px; max-width: 630px; width:100%; overflow: hidden; position: relative; margin: 0 auto;}
.email_plan .ebt{width: 71.56%; margin: 5% auto 3%;}
.email_plan .ebt img{width: 100%;}
.email_plan .in_box{ border: 2px solid #d9d9d9; background: #fff; border-radius: 10px;width:92.85%; font-size: 2.4em; margin: 0 auto;}
.email_plan .in_box .sbt1{ color: #5063ca;width:29.05%; padding-left: 3%; display: inline-block; vertical-align: middle;}
.email_plan .in_box .input1{ border: none; border-left: 2px solid #dbdbdb;width:58.97%; margin:2% 0;padding:1% 0 1% 3%; display: inline-block; vertical-align: middle; }
.email_plan .txt1{color:#777777; text-align: center; margin: 2% 0 5%; font-size: 2.2em;}
.email_plan .try_btn{width:38.25%; margin: 0 auto 5%; display: block;}
.email_plan .try_btn img{width: 100%;}
.close_btn{ text-decoration: none; font-size: 2.5em; color:#777777; border-radius: 50%; border: 1px solid #777777; width:40px;height:40px; line-height: 40px; display:block; text-align: center; position: absolute; right: 5px; top:5px; }

/*其他页面内容面板*/
.ot_con {
    width: 100%;

   /* background-color: #53a2cf;*/
}
.share{
    background: url("../images/bg1.jpg") no-repeat 0 0/100% 100%;
