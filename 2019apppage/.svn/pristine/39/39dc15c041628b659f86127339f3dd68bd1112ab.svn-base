*{
	margin:0;
	padding: 0;
}
html,body{
	width: 100%;
	height: 100%;
}
em{
	font-style: normal;
}
ol,li,ul{
	list-style: none;
}
body{
	background-color: #f0f0f0;
	font-size: 1rem;
}
section{
	width: 100%;
	height: 100%;
	max-width: 640px;
	margin:0 auto;
	position: relative;
}
.topbar{
	background-color: #fff;
	position: fixed;
	font-size: 1.2rem;
	height: 3.5rem;
	line-height: 3.5rem;
	width: 100%;
	padding-left: 5%;
	max-width: 640px;
	z-index: 2;
}
.rulebtn{
    display: inline-block;
    position: absolute;
    color: #5c6bc2;
    right: 2%;
    top: 0.7rem;
    width: 5.5rem;
    height: 2rem;
    line-height: 1.9rem;
    text-align: center;
    border: 1px solid #5c6bc2;
    border-radius: 5px;
}
.bar{
	display: inline-block;
	position: absolute;
	background-color: #5c6bc2;
	width: 0.3rem;
	height: 2rem;
	left: 0;
	top:0.75rem;
}
.content{
	padding-top: 4.5rem;
	position: relative;
}
.tab{
	background-color: #fff;
	border-bottom: 1px solid #f0f0f0;
}
.tab li{
	display: inline-block;
	width: 49%;
	text-align: center;
	height: 3rem;
	font-size: 1.2rem;
	line-height: 3rem;
}
.active{
	color:#5c6bc2;
	border-bottom: 2px solid #5c6bc2;
}
.listbox{
	width: 100%;
	position: relative;
	background-color: #fff;
}
.season_box,.music_box{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
}

.mui-pull-bottom-tips{
	width: 100%;
	text-align: center;
}
.music_box{
	display: none;
}
.mui-content{
	background-color: transparent;
}


/*列表*/
.listbox li{
	height: 6rem;
	text-align: center;
}
.runknum{
	display: block;
	width: 10%;
	line-height: 6rem;
	font-size: 1.5rem;
	font-weight:bolder;
	float: left;
	color:#c4c4c4;
}
.the{
	color:#839cff;
}
.headbox{
	float:left;
	width: 65%;
	height: 100%;
}
.headimg{
	display: block;
	width: 4rem;
	height: 4rem;
	background-repeat: no-repeat;
	background-position: center top;
	background-size: cover;
	background-image: url(../img/zy.png);
	border-radius: 100%;
	margin-top: 1rem;
	float: left;
}
.headbox div{
	text-align: left;
	padding-left: 5rem;
	line-height: 2rem;
    margin-top: 1.1rem;
}
.p1,.p2{
	margin-bottom: 0!important;
}
.p1{
	font-size: 1.2rem!important;
	color:#363d4d!important;
	overflow:hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.p2{
	font-size: 1rem!important;
	color:#808080!important;
}
.votebox{
	width: 25%;
	/* height: 6rem; */
	float: right; 
	margin-top: 1.2rem;
}
.votebtn{
	display: block;
	padding: 0 10%;
	text-align: center;
	border-radius: 5px;
	border:1px solid #596ec2;
	font-size: 1.1rem;
	float: right;
	color:#596ec2;
	height: 2rem;
	line-height: 2rem;
	margin-right: 10%;
}
.votebtn_no{
	display: block;
	padding: 0 10%;
	text-align: center;
	border-radius: 5px;
	border:1px solid #d5d6d7;
	font-size: 1.1rem;
	float: right;
	color:#b1b1b1;
	height: 2rem;
	line-height: 2rem;
	margin-right: 10%;
}
.votebox p{
	font-size: 1rem;
	color:#adadad;
	text-align: right;
	margin-bottom: 0px!important;
	margin-top: 2.5rem;
	margin-right: 10%;
	overflow:hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.listbox ul li{
	border-bottom: 1px solid #c8c7cc;
}
.exchange{
	width: 26%;
	position: absolute;
	bottom: 1%;
	left: 37%;
	z-index: 3;
}
.exchange img{
	width: 100%;
}
.exnum{
    font-size: 1.5rem;
    color: #fff;
    position: absolute;
    bottom: 24%;
    right: 30%;
	font-style: oblique;
}

/*弹窗*/
.exchange_mask,.mask,.recharge_mask{
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	position: fixed;
	background-color: rgba(0,0,0,.4);
	z-index: 3;	
}
.exchange_mask,.recharge_mask{
	display: none;
}
.mask{
	display: none;
}
.changebox{
	background-color: #fff;
	width: 70%;
	height: 25%;
	position: absolute;
	left: 15%;
	top: 35%;
	border-radius: 10px;
}
.c_top{
	width: 100%;
	height: 65%;
	text-align: center;
	padding-top: 11%;
	line-height: 2rem;
}
.c_top p{
	font-size: 1.3rem;
	color:#000;
	margin-bottom: 0;
}
.recharge_mask .c_top{
	padding-top: 16%;
}
.c_btns{
	width: 100%;
	height: 34%;
	border-top:1px solid #c4c4c4;
}
.c_btns button,.s_btns button{
	border:none;
	background-color: transparent;
	width: 47%;
	height: 100%;
	font-size: 1.2rem;
	color:#007aff;
	outline: none;
	display: inline-block;
}
.cancel{
	border-right: 1px solid #c4c4c4!important;
}


.s_votebox{
    background-color: #fff;
    width: 80%;
    height: 19rem;
    position: absolute;
    left: 10%;
    top: 50%;
    margin-top: -9.5rem;
    border-radius: 10px;
}
.s_top{
	width: 100%;
	height: 15rem;
	text-align: center;
	padding-top: 4%;
	line-height: 2rem;
}

.mui-numbox{
	width: 13rem;
	height: 2.5rem;
	background-color:transparent;
	border:none;
	padding: 0 3.5rem;
	margin: 2% 0;
}
.mui-numbox [class*=mui-numbox-btn]{
	background-color: #839cff!important;
	border-radius: 100%!important;
	width: 2.4rem;
	height: 2.4rem;
	border:none;
	font-size: 1.5rem;
	color:#fff;
}
.mui-numbox .mui-numbox-input{
	border:1px solid #ccc!important;
	font-size: 1.3rem;
}
.s_btns{
	width: 100%;
	height: 4rem;
	border-top:1px solid #c4c4c4;
}
.s_top p{
	margin-bottom: 0;
	overflow:hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.s_top h4{
	overflow:hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.s_title{
	font-size: 1.3rem;
	color:#000;
}
.name{
	font-size: 1.5rem;
}
.time,.balance,.notice{
	color:#adadad;
	font-size: 1.1rem;
}
.notice{
	color:#ef634c;
	display: none;
}
.s_texts,.m_texts{
	width: 100%;
	display: none;
}

.mui-btn.mui-active:enabled, .mui-btn:enabled:active, button.mui-active:enabled, button:enabled:active, input[type=button].mui-active:enabled, input[type=button]:enabled:active, input[type=reset].mui-active:enabled, input[type=reset]:enabled:active, input[type=submit].mui-active:enabled, input[type=submit]:enabled:active{
	background-color: transparent;
}